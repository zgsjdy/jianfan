@keyframes goToFrames-79eeece5 {
1% {
    opacity: 0;
    transform: scale(0.3, 0) translateY(30px);
}
100% {
    opacity: 1;
    transform: scale(1, 1) translateY(0px);
}
}
.cvcv.data-v-79eeece5 {
  transform-origin: 50% 100%;
  animation: goToFrames-79eeece5 0.6s;
}
@keyframes goToFramesZC-79eeece5 {
50% {
    filter: blur(0px);
    transform: scale(1.3, 1.3);
}
90% {
    filter: blur(3px);
    transform: scale(1, 1);
}
100% {
    filter: blur(0px);
    transform: scale(1, 1);
}
}
.cvcvZC.data-v-79eeece5 {
  transform-origin: 50% 50%;
  animation: goToFramesZC-79eeece5 0.8s;
}
@keyframes botboxanim-79eeece5 {
1% {
    opacity: 0;
    transform: translateY(100%) scale(1, 0);
}
100% {
    opacity: 1;
    transform: translateY(0%) scale(1, 1);
}
}
.botboxanim.data-v-79eeece5 {
  animation: botboxanim-79eeece5 1.5s;
}
.botbox.data-v-79eeece5 {
  width: 100vw;
  height: 120rpx;
  background-color: #F8F8F8;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  z-index: 999;
  padding-bottom: env(safe-area-inset-bottom);
  /* 当设置为flex: 50%;时，相当于：
  flex-grow: 1;（默认值为 0，设置为 1 表示如果有剩余空间，该项目会等比例放大）。
  flex-shrink: 1;（默认值为 1，表示如果空间不足，该项目会等比例缩小）。
  flex-basis: 50%;（表示在分配多余空间之前，该项目占据的主轴空间为其父容器主轴空间的 50%）。 */
}
.botbox .botcon.data-v-79eeece5 {
