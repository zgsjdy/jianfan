@keyframes botboxanim-fba290dc {
1% {
    background-image: none;
    opacity: 0;
    transform: translateY(-100%) scale(1, 0);
}
100% {
    background-image: none;
    opacity: 1;
    transform: translateY(0%) scale(1, 1);
}
}
.botboxanim.data-v-fba290dc {
  animation: botboxanim-fba290dc 1.5s;
}
@keyframes goToFrames-fba290dc {
1% {
    opacity: 0;
    transform: scale(0.8, 0.8);
}
100% {
    opacity: 1;
    transform: scale(1, 1);
}
}
.cvcv.data-v-fba290dc {
  transform-origin: 50% 100%;
  animation: goToFrames-fba290dc 0.8s;
}
@keyframes selectionInto-fba290dc {
1% {
    opacity: 0;
    transform: scale(0, 0) translateY(-300px);
}
100% {
    opacity: 1;
    transform: scale(1, 1) translateY(0px);
}
}
.selectionInto.data-v-fba290dc {
  animation: selectionInto-fba290dc 0.8s forwards;
}
@keyframes selectionExit-fba290dc {
1% {
    opacity: 1;
    transform: scale(1, 1) translateY(0px);
}
40% {
    opacity: 0.8;
    transform: scale(0.8, 0.8) translateY(-50px);
}
100% {
    opacity: 0;
    transform: scale(0, 0) translateY(-300px);
}
}
.selectionExit.data-v-fba290dc {
  animation: selectionExit-fba290dc 0.8s forwards;
}
.headerbox.data-v-fba290dc {
